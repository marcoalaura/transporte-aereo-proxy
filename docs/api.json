  "definitions": {
    "OP1-Estado": {
      "type": "object",
      "properties": {
        "estado": {
          "type": "string",
          "description": "Estado de la API REST."
        }
      },
      "example": {
        "estado": "El servicio se encuentra disponible"
      }
    },
    "OP1-Despliegue": {
      "type": "object",
      "properties": {
        "version": {
          "type": "string",
          "description": "versión desplegada de la API REST."
        }
      },
      "example": {
        "version": "1.0.0"
      }
    },
    "OP1-Error": {
      "type": "object",
      "properties": {
        "error": {
          "type": "string",
          "description": "Descripción del error al consumir el servicio web."
        }
      },
      "example": {
        "error": "Existen otros itinerarios en las fechas y horas solicitadas."
      }
    },
    "OP1-Itinerario": {
      "type": "object",
      "properties": {
        "fecha_inicio": {
          "type": "date (YYYY-MM-DD)",
          "description": "Fecha de inicio del itinerario a solicitar. [Requerido]"
        },
        "fecha_fin": {
          "type": "date (YYYY-MM-DD)",
          "description": "Fecha final del itinerario a solicitar. [Requerido]"
        },
        "itinerarios": {
          "type": "object",
          "properties": {
            "eqv": {
              "type": "string",
              "description": "Número de matricula de la aeronave, este código tiene el formato 'CP-NNNNNN', esta de acuerdo al registro de la DGAC. [Requerido]"
            },
            "vlo": {
              "type": "string",
              "description": "Número de vuelo codificado por el aeropuerto. [Requerido]"
            },
            "ori": {
              "type": "string",
              "description": "Código IATA del aeropuerto de origen. [Requerido]"
            },
            "des": {
              "type": "string",
              "description": "Código IATA del aeropuerto de destino. [Requerido]"
            },
            "etd": {
              "type": "string",
              "description": "Hora de despegue solicitada en formato 'HH:MM'. [Requerido]"
            },
            "eta": {
              "type": "string",
              "description": "Hora de aterrizaje solicitada en formato 'HH:MM'. [Requerido] Se puede complementar el valor +1 junto a la hora para indicar que esta es una hora del día siguiente. Ejemplo: 05:00+1"
            },
            "dia": {
              "type": "string",
              "description": "Los días solicitados para vuelos bajo el formato '1-2-3-4', donde 1 corresponde al día Lunes y asi los otros días de la semana hasta el código 7. [Requerido]"
            },
            "obs": {
              "type": "string",
              "description": "Observaciones realizadas en el itinerario solicitado. [Opcional]"
            }
          }
        }
      },
      "example": {
        "fecha_inicio": "2018-08-22",
        "fecha_fin": "2018-08-27",
        "itinerarios": [
          {
            "eqv": "CP-2715",
            "vlo": "100",
            "ori": "LPB",
            "des": "VVI",
            "etd": "07:00",
            "eta": "08:00",
            "dia": "1-2-3-4-5-6",
            "obs": "NINGUNA"
          },
          {
            "eqv": "CP-2762",
            "vlo": "101",
            "ori": "VVI",
            "des": "LPB",
            "etd": "07:00",
            "eta": "08:00",
            "dia": "1-2-3-4-5",
            "obs": "NINGUNA"
          }
        ]
      }
    },    
    "OP1-mensajePost": {
      "type": "object",
      "properties": {
        "id_solicitud": {
          "type": "integer",
          "description": "Mensaje del estado de la operación."
