    "/api/itinerario/crear": {
      "post": {
        "tags": [
          "ITINERARIOS"
        ],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "summary": "Registrar Solicitud de Itinerarios",
        "description": "Por medio del siguiente servicio los operadoradores o aerolíneas, podrán realizar las solicitudes de itinerarios directamente según rangos de fechas, horarios y días que precisen.",
        "operationId": "solicitud-itinerario",
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "Operación exitosa.",
            "schema": {
              "$ref": "#/definitions/OP1-mensajePost"
            },
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/definitions/OP1-mensajePost"
                }
              }
            }
          },
          "400": {
            "description": "Error en el registro de los itinerarios.",
            "schema": {
              "$ref": "#/definitions/OP1-errorRegistro"
            },
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/definitions/OP1-errorRegistro"
                }
              }
            }
          }
        },
        "requestBody": {
          "description": "Payload con los datos de itinerario",
          "required": "true",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/definitions/OP1-Itinerario"
              }
            }
          }
        },
        "x-code-samples": [
          {
            "lang": "cURL",
            "source": "curl -X POST   https://aereo.oopp.gob.bo/transporte-proxy/api/itinerario/crear   -H 'Authorization: Bearer {token}'  -d '{ \n   {\n     \"fecha_inicio\": \"2018-08-22\",\n     \"fecha_fin\": \"2018-08-27\",\n     \"itinerarios\": [\n           {\n           \"eqv\": \"CP-2715\",\n           \"vlo\": \"100\",\n           \"ori\": \"LPB\",\n           \"des\": \"VVI\",\n           \"etd\": \"07:00\",\n           \"eta\": \"08:00\",\n           \"dia\": \"1-2-3-4-5-6\",\n           \"obs\": \"NINGUNA\"\n          },\n          {\n           \"eqv\": \"CP-2762\",\n           \"vlo\": \"101\",\n           \"ori\": \"VVI\",\n           \"des\": \"LPB\",\n           \"etd\": \"07:00\",\n           \"eta\": \"08:00\",\n           \"dia\": \"1-2-3-4-5\",\n           \"obs\": \"NINGUNA\"\n          }\n      ]\n   }\n'\n"
          }
        ]
      }
    },
